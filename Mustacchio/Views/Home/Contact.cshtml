
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="contact-content">

    <div class="contact-content-left">

        <h6>APPOINTMENTS</h6>
        <h2>Book it in.</h2>

        <form onsubmit="alert('Appointment Checked in')">
            <input type="text" placeholder="First Name" />
            <input type="text" placeholder="Last Name" />
            <br />
            <input type="text" placeholder="Phone Number" />
            <input type="email" placeholder="Email" />
            <br />
            Date of the appointment: <input type="date" />
            <br />
            <input type="submit" value="Complete"/>
        </form>

    </div>

    <div class="contact-content-right">
        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d30277.627267699598!2d-69.66233710000002!3d18.45177080000001!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sdo!4v1573428400815!5m2!1sen!2sdo" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen=""></iframe>
    </div>
    

</div>

